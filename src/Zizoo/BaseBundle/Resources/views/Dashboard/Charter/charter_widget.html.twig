{% trans_default_domain "labels" %}

<div id="dashboard_charter">
    <div class="avatar">
        {% if charter.logo is not null %}
            <img src="{{ asset(charter.logo|pictureWebPath) }}" alt="image not found" class="large" />
        {% endif %}
    </div>
    <h3>{{ charter.charterName }}</h3>
  
    <p>Captain!</p>
           
        
    <a id="add_boat" href="{{ path('ZizooBoatBundle_Boat_New') }}">Add Boat</a>
</div>

<hr />

<div class="dashboard_navigation">
    <ul class="nav">
      <li class="active"><a href="{{ path('ZizooBaseBundle_Dashboard_CharterDashboard') }}">Dashboard</a></li>
      <li><a href="{{ path('ZizooBaseBundle_Dashboard_CharterInbox') }}">Inbox</a></li>
      <li><a href="{{ path('ZizooBaseBundle_Dashboard_CharterSent') }}">Sent Messages</a></li>
      <li><a href="{{ path('ZizooBaseBundle_Dashboard_CharterProfile') }}">Profile</a></li>
      <li><a href="{{ path('ZizooBaseBundle_Dashboard_CharterBookings') }}">My Bookings</a></li>
      <li><a href="{{ path('ZizooBaseBundle_Dashboard_CharterBoats') }}">My Boats</a></li>
      <li><a href="{{ path('ZizooBaseBundle_Dashboard_CharterPayments') }}">My Payments</a></li>
      <li><a href="{{ path('ZizooBaseBundle_Dashboard_CharterPayoutSettings') }}">Payout Settings</a></li>
    </ul>
    <a href="{{ path('ZizooBaseBundle_Dashboard_UserDashboard', {show_user: 'true'}) }}">Switch to user view</a>
</div>
